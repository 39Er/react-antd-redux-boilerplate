{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "strict": [
            0
        ],
        "arrow-body-style": [
            0
        ],
        "func-names": [
            0
        ],
        "no-param-reassign": [
            0
        ],
        "global-require": [
            0
        ],
        "no-console": [
            0
        ],
        "import/no-dynamic-require": [
            0
        ],
        "object-shorthand": [
            0
        ],
        "prefer-const": [
            0
        ],
        "prefer-template": [
            0
        ],
        "no-use-before-define": [
            "error",
            {
                "functions": false
            }
        ],
        "import/no-extraneous-dependencies": [
            "error",
            {
                "devDependencies": true,
                "optionalDependencies": false,
                "peerDependencies": false
            }
        ]
    },
    "env": {
        "node": true,
        "mocha": true
    }
}